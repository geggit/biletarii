<!DOCTYPE html>
<html>
  <head>
    <title><%= @title || "Biletarii" %></title>
    <%= stylesheet_link_tag    "application" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  
  <body>
    <% flash.each do |key, value| %>
      <div class='flash' id='<%= key %>'>
        <%= value %>
      </div>
    <% end %>

    <h1><%= link_to "Biletarii", root_path %></h1>
    <%= yield %>

  </body>
</html>
